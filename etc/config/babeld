package babeld

config general
	option 'log_file' '/var/log/babeld.log'
	option 'ipv6_subtrees' 'true'

config interface
	option 'ifname' 'wlan0'

config interface                                                               
        option 'ifname' 'wlan1'

config interface                                                               
        option 'ifname' 'eth0.1'

config interface                                                               
        option 'ifname' 'eth0.2'

config interface
	option 'enable_timestamps' 'true'

config filter
	option 'ignore'	'true'
	option 'type'	'redistribute'
	option 'ip'	'0.0.0.0/0'
	option 'eq'	'0'
	option 'proto'	'3'
	option 'action'	'metric 128'

config filter
	option 'ignore'	'true'
	option 'type'	'redistribute'
	option 'local'	'true'
